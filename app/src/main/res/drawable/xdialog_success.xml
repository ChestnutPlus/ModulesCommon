<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--布局-->
    <item>
        <shape android:shape="rectangle">
            <size android:height="70dp" android:width="70dp"/>
            <solid android:color="#009b9595"/>
        </shape>
    </item>

    <!--环-->
    <item>
        <shape android:shape="ring" android:thickness="3dp" android:useLevel="false" android:innerRadius="30.5dp">
            <solid android:color="@android:color/holo_green_light"/>
        </shape>
    </item>

    <!--左上角-右下角-->
    <item android:top="44dp" android:bottom="20dp" android:left="10dp" android:right="38dp">
        <rotate android:fromDegrees="45" android:toDegrees="45" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/holo_green_light"/>
                <corners android:bottomLeftRadius="4dp" android:bottomRightRadius="2dp"
                    android:topRightRadius="4dp" android:topLeftRadius="4dp"/>
            </shape>
        </rotate>
    </item>

    <!--左下角-右上角-->
    <item android:top="37dp" android:bottom="27dp" android:left="20dp" android:right="2dp">
        <rotate android:fromDegrees="315" android:toDegrees="315" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/holo_green_light"/>
                <corners android:bottomLeftRadius="3dp" android:bottomRightRadius="4dp"
                    android:topRightRadius="4dp" android:topLeftRadius="0dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>