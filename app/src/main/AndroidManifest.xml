<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chestnut.RouterArchitecture.ModulesCommon">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".base.CommonApplication"
        android:icon="@drawable/li_bao_en"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestActivity" />
        <activity
            android:name=".DiyTestSurfaceViewActivity"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />
        <activity android:name=".PayActivity" />
        <activity android:name=".RecordPlayActivity" />

        <service
            android:name=".TestBgService"
            android:enabled="true"
            android:exported="true"
            android:process=":TestBgService">
            <intent-filter>
                <action android:name="com.hy.TestBgService" />
            </intent-filter>
        </service>

        <activity android:name=".OemHWLActivity" />
        <activity android:name=".DemoInstallUninstallActivity" />
        <activity android:name=".view.activity.CommonHomeActivity"></activity>
    </application>

</manifest>